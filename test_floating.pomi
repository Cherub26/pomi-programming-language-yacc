// Test float handling
DEFINE s = 9.8;
PRINT(s);  // Should print 9.80

// Float addition
DEFINE f = s + 1.9;
PRINT(f);  // Should print 11.70

// Call function with different combinations
DEFINE a = 5+3; // Should print 8
DEFINE b = 5.5 + 3; // Should print 8.50
DEFINE c = 5 + 3.5; // Should print 8.50
DEFINE d = 5.555 + 3.555; // Should print 9.11

PRINT(a);
PRINT(b);
PRINT(c);
PRINT(d);

// Test other operators with floats
PRINT(s * 2.0);  // multiplication: 19.60
PRINT(f / 2.0);  // division: 5.85
PRINT(f - s);    // subtraction: 1.90